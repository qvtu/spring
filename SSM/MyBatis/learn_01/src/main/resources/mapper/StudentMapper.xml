<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rzhylj.dao.StudentDAO">
    <select id="searchStu" parameterType="Integer" resultType="com.rzhylj.entity.Student">
        select *
        from student
        where stuId = #{stuId};
    </select>
    <select id="searchCls" parameterType="Integer" resultType="com.rzhylj.entity.Student">
        select *
        from student
        where stuCls = #{stuCls};
    </select>
</mapper>